{
	"geode": "5.0.0-beta.2",
	"gd": {
		"win": "2.2081",
		"android": "2.2081",
		"mac": "2.2081",
		"ios": "2.2081"
	},
	"id": "legowiifun.API_test",
	"name": "Test for API mods",
	"version": "v3.0.0",
	"developer": "legowiifun",
	"description": "A test for various API mods developed by me",
	"resources": {
		"sprites": [
			"resources/*.png"
		]
	},
	"links": {
		"source": "https://github.com/legowiifun/Geometry-Dash-API-Test"
	},
	"settings": {
		"cheat-api-title": {
			"type":"title",
			"name":"Cheat API"
		},
		"enable-cheat-api": {
			"type": "string",
			"default": "dispatch events",
			"one-of": ["off", "dispatch events", "direct linking"],
			"name": "Setting for cheat API"
		},
		"stat-display-title": {
			"type":"title",
			"name":"Stat Display"
		},
		"enable-stat-display": {
			"type": "bool",
			"default": true,
			"name": "Setting for the Stat Display API showcase"
		},
		"hide-ui-title": {
			"type":"title",
			"name":"Hide UI"
		},
		"enable-toggle-UI": {
			"type":"bool",
			"default":true,
			"name": "Setting for the direct linking Hide UI showcase"
		},
		"enable-hide-ui-dispatch-test": {
			"type":"bool",
			"default":true,
			"name": "Setting for the showcase of the Hide UI dispatch events"
		}
	},
	"tags": ["offline","interface","utility", "developer", "api"],
	"dependencies": {
		"legowiifun.cheat_api": {
			"version": ">=1.2.3",
			"required": true
		},
		"legowiifun.stat_display": {
			"version": ">=2.1.1",
			"required": false
		},
		"legowiifun.hide_ui": {
			"version": ">=1.3.0",
			"required": true
		}
	}
}
